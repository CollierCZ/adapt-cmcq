<div class="cmcq-inner component-inner clearfix" role="region" aria-label="{{_globals._components._cmcq.ariaRegion}}">
    {{> component this}}
    <div class="cmcq-widget component-widget clearfix {{#unless _isEnabled}} disabled {{#if _isInteractionComplete}} complete submitted show-user-answer {{#if _isCorrect}}correct{{/if}} {{/if}} {{/unless}}">
        {{#each _items}}
        <div class="cmcq-item component-item {{#unless ../_isEnabled}}{{#if ../_canShowMarking}}{{#if _isCorrect}}correct{{else}}incorrect{{/if}}{{/if}}{{/unless}} item-{{@index}}">
            <input type="checkbox" id="{{../_id}}-{{@index}}" aria-labelledby="{{../_id}}-{{@index}}-aria"{{#unless ../_isEnabled}} disabled{{/unless}} aria-label="{{a11y_normalize text}} {{_graphic.alt}}"/>
            <label aria-hidden="true" id="{{../_id}}-{{@index}}-aria" for="{{../_id}}-{{@index}}" class="{{#unless ../_isEnabled}}disabled {{/unless}} component-item-color component-item-text-color component-item-border {{#if _isSelected}}selected{{/if}} a11y-ignore" tabindex="-1">



                <div class="cmcq-item-checkbox">
                    <div class="cmcq-item-state">
                        <div class="cmcq-item-icon cmcq-answer-icon {{#if ../_isRadio}}radio{{else}}checkbox{{/if}} icon"></div>
                        <div class="cmcq-item-icon cmcq-correct-icon icon icon-tick"></div>
                        <div class="cmcq-item-icon cmcq-incorrect-icon icon icon-cross"></div>
                    </div>
                    <div class="cmcq-item-inner">
                      <div class="cmcq-item-title">{{{title}}}</div>
                        <pre><code class="language-{{code.lang}}">{{code.src}}</code></pre>
                    </div>
                </div>
            </label>
        </div>
        {{/each}}
    </div>
    <div class="buttons">
    </div>
</div>
